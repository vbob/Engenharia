#include <main.h>

void main()
{
   while(TRUE)
   {
      output_low(PIN_C2);
      for (int i = 0; i < 30; i++) {
         for (int j = 0; j < 1000; j++) {
            output_high(PIN_C2);
            delay_us(1000*i);
            output_low(PIN_C2);
            delay_us(1000-(100*i));
         }
      }
   }

}
