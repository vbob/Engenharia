#include <main.h>

void main()
{
   while(TRUE)
   {
      for (int i = 0; i < 30; i++) {
         for (int j = 0; j < 100; j++) {
            output_high(PIN_C2);
            delay_us(i*100);
            output_low(PIN_C2);
            delay_us(100-(i));
         }
      }
   }

}
